    <SwitchPreference
        android:defaultValue="true"
        android:key="agreeMarketingAlarm"
        android:title="이벤트 혜택 알림"
        android:summary="광고 및 마케팅 정보 알림"
        app:iconSpaceReserved="false"/>

</PreferenceScreen>