<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout203"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment_203">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <Button
                android:id="@+id/fragment203_myInfo"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"
                android:background="@drawable/round_corner_button"
                android:drawableRight="@drawable/ic_custom_chevron_right_24dp"
                android:paddingRight="16dp"
                android:text="내 정보 수정"
                android:textSize="18dp" />

            <Button
                android:id="@+id/fragment203_myOutfits"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"
                android:background="@drawable/round_corner_button"
                android:drawableRight="@drawable/ic_custom_chevron_right_24dp"
                android:paddingRight="16dp"
                android:text="내 게시글 관리"
                android:textSize="18sp" />

            <Button
                android:id="@+id/fragment203_settings"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/round_corner_button"
                android:drawableRight="@drawable/ic_custom_chevron_right_24dp"
                android:paddingRight="16dp"
                android:text="환경설정"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/fragment203_logo_with_title"
                android:layout_width="200dp"
                android:layout_height="120dp"
                android:layout_gravity="center"
                android:layout_marginTop="80dp"
                android:src="@drawable/logo_with_title" />

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>