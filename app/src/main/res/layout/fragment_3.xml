<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="56dp"
    tools:context=".Fragment_Btm">

    <ImageButton
        android:id="@+id/LeftBtmBtn"
        android:layout_width="54dp"
        android:layout_height="54dp"
        android:layout_marginStart="45dp"
        android:background="@drawable/round_corner_button_clicked"
        android:padding="12dp"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_custom_leftbtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/MidBtmBtn"
        android:layout_width="54dp"
        android:layout_height="54dp"
        android:background="@drawable/round_corner_button_clicked"
        android:padding="12dp"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_custom_midbtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/RightBtmBtn"
        app:layout_constraintStart_toEndOf="@id/LeftBtmBtn"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/RightBtmBtn"
        android:layout_width="54dp"
        android:layout_height="54dp"
        android:layout_marginEnd="45dp"
        android:background="@drawable/round_corner_button_clicked"
        android:padding="12dp"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_custom_rightbtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.526" />

    <View
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="1dp" />
</androidx.constraintlayout.widget.ConstraintLayout>